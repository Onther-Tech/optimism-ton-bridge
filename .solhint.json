{
  "extends": "solhint:recommended",
  "rules": {
    "avoid-suicide": "error",
    "avoid-sha3": "error",
    "code-complexity": ["warn", 7],
    "compiler-version": "off",
    "max-states-count": ["error", 18],
    "max-line-length": ["warn", 145],
    "not-rely-on-time": "warn",
    "quotes": ["error", "double"]
  }
}
